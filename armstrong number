class Learncoding {
    public static void main(String[] args){
        int n=123,sum=0,temp=n,cube;
        while(n>0){
            int digit =n%10;
            cube=digit*digit*digit;
            sum=sum+cube;
            n=n/10;
        }
        if(temp==sum)
        {
            System.out.println("Armstrong number");
        }
        else
        {
            System.out.println("not Armstrong number");
        }
    }}
